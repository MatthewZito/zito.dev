{
  "name": "zito-dot-dev",
  "appName": "zito.dev",
  "version": "0.0.1",
  "author": "Matthew Zito (goldmund)",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint \"src/**/*.{js,vue}\" --no-fix",
    "lint:css": "stylelint \"src/**/*.{css,scss,vue}\"",
    "lint:fix": "concurrently \"yarn lint --fix\" \"yarn lint:css --fix\"",
    "test:unit": "tape -r \"esm\" \"src/**/*.test.js\"",
    "new": "HYGEN_TMPLS=generators hygen new"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "yarn lint"
    ],
    "src/**/*.{vue,css,scss}": [
      "yarn lint:css"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/vue-fontawesome": "3.0.0-3",
    "@rollup/plugin-eslint": "8.0.1",
    "@vitejs/plugin-legacy": "1.3.2",
    "@vueuse/head": "0.5.1",
    "coalesce-x": "1.1.0",
    "http-factory": "1.3.0",
    "js-heuristics": "1.1.0",
    "markdown-it": "12.0.6",
    "masonry-layout": "4.2.2",
    "nprogress": "0.2.0",
    "vite-plugin-md": "0.11.0",
    "vite-plugin-pages": "0.18.0-beta.1",
    "vue": "^3.0.5",
    "vue-router": "4.0.6",
    "vuex": "4.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-eslint": "10.1.0",
    "concurrently": "6.2.1",
    "eslint": "7.25.0",
    "eslint-plugin-vue": "7.9.0",
    "esm": "3.2.25",
    "husky": "4.3.0",
    "hygen": "6.0.4",
    "lint-staged": "10.5.4",
    "markdown-it-prism": "2.2.0",
    "postcss-import": "14.0.1",
    "postcss-preset-env": "6.7.0",
    "prismjs": "1.23.0",
    "pug": "3.0.2",
    "rollup-plugin-node-builtins": "2.1.2",
    "sass": "1.32.11",
    "stylelint": "13.13.1",
    "stylelint-at-rule-no-children": "0.3.1",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-images": "2.0.1",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "tape": "5.3.1",
    "vite": "2.2.4",
    "vite-plugin-svg-icons": "0.5.0"
  }
}
