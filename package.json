{
  "name": "zito.dev",
  "version": "2.0.0",
  "description": "Matthew Zito's personal blog site",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatthewZito/zito.dev.git"
  },
  "author": "Matthew Zito <matthewtzito@gmail.com> (goldmund)",
  "main": "index.js",
  "scripts": {
    "dev": "gatsby develop -p 3000 -o",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier --write \"src/**/*.{ts,tsx,md,css,scss}\"",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx,.json . --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss,ts,tsx}\" --fix",
    "test:unit": "jest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn typecheck -- --watch",
    "test:e2e:dev": "CYPRESS_SUPPORT=y CYPRESS_BASE_URL=http://localhost:3000 CYPRESS_STAGE=dev start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:open",
    "test:e2e:ci": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:run"
  },
  "bugs": {
    "url": "https://github.com/MatthewZito/zito.dev/issues"
  },
  "homepage": "https://github.com/MatthewZito/zito.dev#readme",
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "yarn format",
      "yarn lint"
    ],
    "src/**/*.{css,scss}": [
      "yarn format",
      "yarn lint:css"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
